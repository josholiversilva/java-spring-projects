type Drink {
    id: ID!
    name: String!
    sweetness: Int!
    rating: Float!
    restaurant_id: Int!
}

type Query {
    findAllDrinks: [Drink]!
    findDrink(id: Int): Drink
}

type Mutation {
    createDrink(
        name: String,
        sweetness: Int,
        rating: Float,
        restaurant_id: Int
    ) : Drink
}